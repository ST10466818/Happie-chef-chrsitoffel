{
  "plugins": [
    "expo-font"
  ]
  ,   
  "expo": {
    "name": "Happie",
    "slug": "Happie",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/Happie-icon.jpeg",
    "userInterfaceStyle": "light",
    "newArchEnabled": true,
    "splash": {
      "image": "./assets/splash-icon.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      },
      "edgeToEdgeEnabled": true,
      "predictiveBackGestureEnabled": false
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "description": "app that allows chef christoffel to add and remove menu items from his digital menu board"
  }
}

